{
    "name": "@xalabaas/shared",
    "version": "0.2.0",
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./src/index.ts",
            "types": "./src/index.ts"
        },
        "./providers": {
            "import": "./src/providers/index.ts",
            "types": "./src/providers/index.ts"
        },
        "./hooks": {
            "import": "./src/hooks/index.ts",
            "types": "./src/hooks/index.ts"
        },
        "./lib": {
            "import": "./src/lib/index.ts",
            "types": "./src/lib/index.ts"
        },
        "./types": {
            "import": "./src/types/index.ts",
            "types": "./src/types/index.ts"
        },
        "./constants": {
            "import": "./src/constants.ts",
            "types": "./src/constants.ts"
        },
        "./navigation": {
            "import": "./src/navigation.ts",
            "types": "./src/navigation.ts"
        },
        "./features": {
            "import": "./src/features/index.ts",
            "types": "./src/features/index.ts"
        },
        "./features/*": {
            "import": "./src/features/*/index.ts",
            "types": "./src/features/*/index.ts"
        }
    },
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "typecheck": "tsc --noEmit"
    },
    "devDependencies": {
        "tsup": "^8.0.0",
        "typescript": "^5.3.0"
    }
}